<% include homepartials/homehead%>
        
        <div class="container">
            <div class="row">
                <div class="col s12 m12 l6 offset-l3">
                    <div class="card white darken-4 mt-6 border z-depth-0">
                        <div class="card-content">
                            <div class="center">
                                <h5 class="uppercase ">Sign up</h5>
                                <div class="divider light-blue"></div>
                            </div>
                        </div>
                        <div class="card-content">
                            <form action="/student/signup" method="POST" class="row">
                                <div class="input-field col s6">
                                    <input placeholder="" id="Surname" name="name" type="text" class="validate">
                                    <label for="Surname">FULL NAME</label>
                                </div>


                                <div class="input-field col s6">
                                    <input placeholder="" type="text" name="dept" id="dept" class="autocomplete">
                                    <label for="Faculty">DEPARTMENT</label>
                                </div>

                                <div class="input-field col s6">
                                    <input placeholder="" id="Surname" name="matric" type="number" data-length="9" class="validate">
                                    <label for="Surname">MATRIC NUMBER</label>
                                </div>


                                <div class="input-field col s6">
                                    <input placeholder="" name="email" id="Student Email" type="email" class="validate">
                                    <label for="Student Email">STUDENT EMAIL</label>
                                </div>


                                <div class="input-field col s12">
                                    <textarea id="textarea1" name="address" class="materialize-textarea"></textarea>
                                    <label for="textarea1">HOUSE ADDRESS</label>
                                </div>
  
                                <div class="input-field col s12">
                                    <input placeholder="" id="surname" name="password" type="password" class="validate">
                                    <label for="surname">PASSWORD</label>
                                  </div>
                                  <div class="input-field col s12">
                                    <input placeholder="" id="surname" name="confirmpassword" type="password" class="validate">
                                    <label for="surname">COMFORM PASSWORD</label>
                                  </div>


                                <div class="center input-field col s12">
                                    <input class="btn  light-blue  waves-effect" value="SIGN UP" type="submit" />
                                    
                                    
                                </div>
                            </form>
                         
                            <br>
                            <div class="divider light-blue"></div>
                            <br>
                            <div class="center">
                                <h6 class="uppercase ">already have an account? <a class="" href="/login">login</a></h6>
                            </div>
                            <!-- </div> -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <% include homepartials/homefooter %>
        