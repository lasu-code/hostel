          <div class="card-content">
            <div class="center">
              <h5 class="uppercase ">login</h5>
              <div class="divider light-blue"></div>
            </div>
          </div>
          <div class="card-content">
            <form action="/student/login" method="post" class="row">
              <div class="input-field col s12">
                <input placeholder="" id="surname" type="text" name="name"  class="validate">
                <label for="matric_number">NAME</label>
              </div>

              <div class="input-field col s12">
                <input placeholder="" id="surname" type="password" name="password" class="validate">
                <label for="surname">PASSWORD</label>
              </div>
              <div class="input-field col s12">
                <input placeholder="" id="matric_number" type="number"  data-length="9" name="matric" class="validate">
                <label for="surname">MATRIC NUMBER</label>
              </div>
              

              <div class="input-field col s12">
                <input placeholder="" id="surname" value="SUBMIT" type="submit" class="validate">
               
              </div>
            </form>
